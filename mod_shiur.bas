Public Function Chumash(date_in As hdate) As String
    Dim current As parshah
    Dim shabbos As hdate
    Dim st As Integer
    
    current = NOPARSHAH
    shabbos = date_in
    
    Do
        current = GetParshah(shabbos)
        If current = NOPARSHAH Then
            HDateAddDay shabbos, 7 - shabbos.wday
        End If
    Loop While current = NOPARSHAH
    
    If current = BERESHIT Then
        If shabbos.EY Then
            st = 22
        Else
            st = 23
        End If
        
        If date_in.day < st Then
            current = VZOT_HABERACHAH
        ElseIf date_in.day = st Then
            If date_in.wday = 1 Then
                Chumash = VBA.Strings.Format("פרשת %s\n%s עם פירש״י", ParshahFormat(BERESHIT), NumToWDay(date_in, False))
                Exit Function
            ElseIf date_in.wday = 0 Then
                Chumash = VBA.Strings.Format("פרשת %s\n%s עם פירש״י", ParshahFormat(VZOT_HABERACHAH), NumToWDay(date_in, False))
                Exit Function
            Else
                'Chumash = VBA.Strings.Format("parashat %s \n %s - shvii,\n %s \nrishon - %s with rashi", parshahformat(VZOT_HABERACHAH), NumToWDay(date_in, False), parshahformat(BERESHIT), NumToWDay(date_in, False))
                Exit Function
            End If
        End If
    End If
    
    Chumash = VBA.Strings.Format("פרשת %s\n%s עם פירש״י", ParshahFormat(current), NumToWDay(date_in, False))
End Function

Public Function Tehillim(date_in As hdate) As String
    Dim current As Integer
    If shiur_ready = False Then init_shiur
    current = date_in.day
    If current = 29 And LastDayOfHebrewMonth(date_in.month, date_in.year) <> 30 Then
        current = 0
    End If
    Tehillim = TehillimArray(current)
End Function
Private Function Tanya(date_in As hdate, ByRef buffer As String) As Integer
    Dim last As Boolean
    Dim leap As Integer
    Dim current As Integer
    Dim shuir As String
    'todo wip
    last = False
    
    If date_in.month = 7 Or date_in.month = 8 Or (date_in.month = 9 And date_in.day < 19) Then
        last = True
    End If
    
    leap = HebrewLeapYear(IIf(last, date_in.year - 1, date_in.year))
    current = date_in.day
    
    If current = 29 And LastDayOfHebrewMonth(date_in.month, date_in.year) <> 30 Then
        current = 0
    End If
    
    If leap Then
        'todo shuir = TanyaArrayLeap(date_in.month - 1, current)
    Else
        'todo shuir = TanyaArrayRegular(date_in.month - 1, current)
    End If
    
    Tanya = VBA.Strings.Format("{0}{1}{2}", "תניא", vbCrLf, shuir)
End Function

Public Function GetHalacha(date_in As hdate) As String
    Dim current As Long
    Dim days As Long
    Dim str_parts() As String
    If shiur_ready = False Then init_shiur
        days = DateDiff("d", #8/16/1954#, (HDateGregorian(date_in))) 'days since 1rd cycle
        current = days Mod 1613
        If days < 0 Then
            GetHalacha = "-"
            Exit Function
        End If
        If current = 1612 Then
            GetHalacha = halachayomi(403)
        Else
            str_parts = Split(halachayomi(Fix(current / 4)), ";")
            GetHalacha = str_parts(current Mod 4)
        End If
End Function

Public Function GetRambam(date_in As hdate, daily_chapter As Boolean) As String
    Dim current As Long
    Dim days As Long
    
    If shiur_ready = False Then init_shiur
    If daily_chapter Then
        'days since start of machzor 1
        days = DateDiff("d", #4/29/1984#, (HDateGregorian(date_in)))
        current = days Mod 1017  '= 1000 chapters + hagadah +  11 days for hakdamah  + 4 days for seder tfila
        If days >= 0 Then
            GetRambam = rambamarray1chp(current)
        Else
            GetRambam = "-"
        End If
    Else
        days = ((HebrewCalendarElapsedDays(date_in.year) + date_in.dayOfYear) - 2097823)
        current = days Mod 339
        If days >= 0 Then
            GetRambam = rambamarray3chp(current)
        Else
            GetRambam = "-"
        End If
    End If
    'Rambam = VBA.Strings.Format("{0}{1}{2}", "רמב״ם", vbCrLf, rambamarray3chp(current))
End Function

